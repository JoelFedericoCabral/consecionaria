#: env/lib/python3.10/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Opciones avanzadas"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Páginas estáticas"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:11
#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Ejemplo: “/about/contact/”. Asegúrese de tener barras al principio y al "
"final."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Este valor solo puede contener letras, números, puntos, guiones bajos o "
"medios, barras o tildes."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "Ejemplo: \"/about/contact”. Asegúrese de tener una barra al principio."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "A la URL le falta la barra inicial."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "A la URL le falta la barra final."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr " En el sitio %(site)s ya hay una pagina estática con la url %(url)s"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "título"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "contenido"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "habilitar comentarios"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nombre de plantilla"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Ejemplo: \"flatpages/contact_page.html\". Si no se proporciona, el sistema "
"utilizará \"flatpages/default.html\"."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "Se requiere registro"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr "Si está marcado, sólo los usuarios registrados podrán ver la página."

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "sitios"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "página estática"

#: env/lib/python3.10/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "páginas estáticas"
